<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="SimulationAndModel.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:fiveExercise="clr-namespace:SimulationAndModel.Features.FiveExercise"
    xmlns:fourExercise="clr-namespace:SimulationAndModel.Features.FourExercise"
    xmlns:oneExercise="clr-namespace:SimulationAndModel.Features.OneExercise"
    xmlns:threeExercise="clr-namespace:SimulationAndModel.Features.ThreeExercise"
    xmlns:twoExercise="clr-namespace:SimulationAndModel.Features.TwoExercise"
    Title="SimulationAndModel">

    <ShellContent
        Title="OneExercise"
        ContentTemplate="{x:DataTemplate oneExercise:OneExercisePage}"
        Route="OneExercisePage" />

    <ShellContent
        Title="TwoExercise"
        ContentTemplate="{x:DataTemplate twoExercise:TwoExercisePage}"
        Route="TwoExercisePage" />

    <ShellContent
        Title="ThreeExercise"
        ContentTemplate="{x:DataTemplate threeExercise:ThreeExercisePage}"
        Route="ThreeExercisePage" />

    <ShellContent
        Title="FourExercise"
        ContentTemplate="{x:DataTemplate fourExercise:FourExercisePage}"
        Route="FourExercisePage" />

    <ShellContent
        Title="FourExercise"
        ContentTemplate="{x:DataTemplate fiveExercise:FiveExercisePage}"
        Route="FiveExercisePage" />
</Shell>
