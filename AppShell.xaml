<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="SimulationAndModel.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:oneExercise="clr-namespace:SimulationAndModel.Features.OneExercise"
    xmlns:twoExercise="clr-namespace:SimulationAndModel.Features.TwoExercise"
    Title="SimulationAndModel">

    <ShellContent
        Title="One Exercise"
        ContentTemplate="{x:DataTemplate oneExercise:OneExercisePage}"
        Route="OneExercisePage" />

    <ShellContent
        Title="TwoExercise"
        ContentTemplate="{x:DataTemplate twoExercise:TwoExercisePage}"
        Route="TwoExercisePage" />
</Shell>
